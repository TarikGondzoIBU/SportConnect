            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="@dimen/_354pxh"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_screenshot2023_678x354}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_screenshot2023_678x354"/>
    </LinearLayout>
