<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="registerVM" type="com.harissapplication.app.modules.register.data.viewmodel.RegisterVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear2Register"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleindigo_900_bf">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackyourparagraph"
            android:layout_height="@dimen/_436pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageYourparagraph"
                android:layout_gravity="start"
                android:layout_height="@dimen/_436pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_yourparagraph}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_yourparagraph"/>
            <LinearLayout android:id="@+id/linearColumnrectangleone"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_23pxv"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:id="@+id/frameStackrectangleone"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_41pxv"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginStart="@dimen/_34pxh" android:layout_width="@dimen/_243pxh">
                    <ImageView android:id="@+id/imageRectangleOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_41pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle1"/>
                    <LinearLayout android:id="@+id/linearRowlogin"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLogIn"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_11pxv"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_weight="0.27"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{registerVM.registerModel.txtLogIn}"
                            style="@style/txtInterregular16" tools:text="@string/lbl_log_in"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnSignUp"
                            android:layout_height="@dimen/_40pxv"
                            android:layout_marginStart="@dimen/_38pxh"
                            android:layout_width="@dimen/_129pxh"
                            android:paddingBottom="@dimen/_10pxv"
                            android:paddingEnd="@dimen/_30pxh"
                            android:paddingStart="@dimen/_30pxh"
                            android:paddingTop="@dimen/_10pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_sign_up"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded" tools:text="@string/lbl_sign_up"/>
                    </LinearLayout>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStacklineone"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_16pxv"
                    android:layout_marginTop="@dimen/_49pxv" android:layout_width="@dimen/_327pxh">
                    <View android:background="@color/red_600"
                        android:id="@+id/lineLineOne"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_327pxh"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEnterEmailor"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{registerVM.registerModel.txtEnterEmailor}"
                        style="@style/txtInterregular13" tools:text="@string/msg_enter_email_or"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtPassword"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_28pxh"
            android:layout_marginStart="@dimen/_28pxh"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{registerVM.registerModel.txtPassword}"
            style="@style/txtInterregular13" tools:text="@string/lbl_password"/>
        <View android:background="@color/red_600"
            android:id="@+id/lineLineTwo"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_28pxh"
            android:layout_marginStart="@dimen/_28pxh"
            android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_327pxh"/>
        <LinearLayout android:id="@+id/linearColumnforgotpassword"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_39pxv"
            android:layout_marginEnd="@dimen/_28pxh"
            android:layout_marginStart="@dimen/_28pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtForgotPassword"
                android:layout_gravity="end"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginStart="@dimen/_222pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{registerVM.registerModel.txtForgotPassword}"
                style="@style/txtInterregular12" tools:text="@string/msg_forgot_password"/>
            <FrameLayout android:id="@+id/frameStacklinethree"
                android:layout_gravity="start"
                android:layout_height="@dimen/_16pxv"
                android:layout_marginTop="@dimen/_38pxv" android:layout_width="@dimen/_327pxh">
                <View android:background="@color/red_600"
                    android:id="@+id/lineLineThree"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_327pxh"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtConfirmpasswor"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{registerVM.registerModel.txtConfirmpasswor}"
                    style="@style/txtInterregular13" tools:text="@string/msg_confirm_passwor"/>
            </FrameLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnSignUpOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_85pxh"
                android:layout_marginStart="@dimen/_85pxh"
                android:layout_marginTop="@dimen/_85pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_9pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@string/lbl_sign_up"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_sign_up"/>
            <TextView android:ellipsize="end" android:id="@+id/txtOR"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_85pxh"
                android:layout_marginStart="@dimen/_85pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{registerVM.registerModel.txtOR}"
                style="@style/txtInterregular13" tools:text="@string/lbl_or"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowellipseone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_85pxh"
                android:layout_marginStart="@dimen/_85pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackellipseone"
                    android:layout_height="@dimen/_43pxv" android:layout_width="@dimen/_48pxh">
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_24"
                        android:id="@+id/viewEllipseOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_43pxv" android:layout_width="@dimen/_48pxh"/>
                    <ImageView android:id="@+id/imageGoogleGLogo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_39pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_googleglogo"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_googleglogo"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackellipsetwo"
                    android:layout_height="@dimen/_43pxv"
                    android:layout_marginStart="@dimen/_28pxh" android:layout_width="@dimen/_48pxh">
                    <View
                        android:background="@drawable/rectangle_bg_white_a700_radius_24"
                        android:id="@+id/viewEllipseTwo"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_43pxv" android:layout_width="@dimen/_48pxh"/>
                    <ImageView android:id="@+id/image2021Facebooki"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_39pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_4pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_39pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_2021facebooki"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_2021facebooki"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
